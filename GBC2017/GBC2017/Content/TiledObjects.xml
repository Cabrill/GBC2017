<?xml version="1.0"?>
<objecttypes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <objecttype name="BasicAlien">
    <property name="MaximumHealth" type="float" default="30" />
    <property name="Speed" type="float" default="125" />
    <property name="IsRangedAttacker" type="bool" />
    <property name="MeleeAttackDamage" type="float" default="15" />
    <property name="RangedAttackDamage" type="float" default="10" />
    <property name="ProjectileSpeed" type="float" default="300" />
    <property name="SecondsBetweenRangedAttack" type="float" default="2" />
    <property name="SecondsBetweenMeleeAttack" type="float" />
    <property name="RangedRadius" type="float" default="250" />
    <property name="MeleeRadius" type="float" />
  </objecttype>
  <objecttype name="LaserTurretProjectile">
    <property name="DamageInflicted" type="float" default="5" />
  </objecttype>
  <objecttype name="RangedEnemyProjectile">
    <property name="DamageInflicted" type="float" />
  </objecttype>
  <objecttype name="LaserTurret">
    <property name="AttackDamage" type="float" default="3" />
    <property name="EnergyBuildCost" type="int" default="100" />
    <property name="CarbonBuildCost" type="int" default="300" />
    <property name="SecondsBetweenFiring" type="float" default="0.5" />
    <property name="EnergyCostToFire" type="float" default="3" />
    <property name="RangedRadius" type="float" default="400" />
    <property name="ProjectileSpeed" type="float" default="300" />
    <property name="MaximumHealth" type="float" default="50" />
    <property name="EnergyRequiredPerSecond" type="int" default="0" />
    <property name="InternalBatteryMaxStorage" type="int" default="15" />
  </objecttype>
  <objecttype name="SolarPanels">
    <property name="EnergyBuildCost" type="int" default="200" />
    <property name="CarbonBuildCost" type="int" default="300" />
    <property name="MaximumHealth" type="float" default="30" />
    <property name="EnergyRequiredPerSecond" type="int" default="0" />
    <property name="InternalBatteryMaxStorage" type="int" default="0" />
    <property name="EnergyProducedPerSecond" type="float" default="10" />
  </objecttype>
  <objecttype name="Home">
    <property name="EnergyBuildCost" type="int" />
    <property name="CarbonBuildCost" type="int" />
    <property name="MaximumHealth" type="float" />
    <property name="EnergyRequiredPerSecond" type="int" />
    <property name="InternalBatteryMaxStorage" type="int" default="0" />
  </objecttype>
  <objecttype name="Battery">
    <property name="EnergyBuildCost" type="int" default="100" />
    <property name="CarbonBuildCost" type="int" default="300" />
    <property name="MaximumHealth" type="float" default="50" />
    <property name="InternalBatteryMaxStorage" type="int" default="100" />
    <property name="EnergyRequiredPerSecond" type="int" default="0" />
  </objecttype>
</objecttypes>